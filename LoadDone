-- Scripts:

local function MMRBVN_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	wait(6)
	script.Parent.Visible = true
	wait(4)
	script.Parent.Visible = false
end
coroutine.wrap(MMRBVN_fake_script)()
